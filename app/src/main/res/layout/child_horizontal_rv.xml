<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_childHorizontal_Item"
    android:layout_width="250dp"
    android:layout_height="wrap_content">

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/imgView"
        android:layout_width="240dp"
        android:layout_height="130dp"
        android:adjustViewBounds="true"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:shapeAppearanceOverlay="@style/normal_shapeable"
        tools:src="@drawable/ride" />

    <TextView
        android:id="@+id/imgLbl"
        style="@style/normaltxt_14spSemibold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        android:layout_marginBottom="3dp"
        android:textColor="@color/newblack2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/imgView"
        app:layout_constraintStart_toStartOf="@id/imgView"
        app:layout_constraintTop_toBottomOf="@id/imgView"
        tools:text="60 Min Adv Rhythm Ride #261" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6pt"
        android:layout_marginBottom="6dp"
        app:layout_constraintBottom_toBottomOf="@+id/imgView"
        app:layout_constraintStart_toStartOf="@+id/imgView"
        app:srcCompat="@drawable/lock_img" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginEnd="6dp"
        android:background="@drawable/drawable_common_rectangle"
        android:backgroundTint="@color/black"
        android:fontFamily="@font/segoeui_regular"
        android:gravity="center"
        android:paddingStart="12dp"
        android:paddingEnd="12dp"
        android:textColor="@color/white"
        android:textSize="11sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        app:layout_constraintEnd_toEndOf="@+id/imgView"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:layout_constraintVertical_bias="0.0"
        tools:text="21:29" />


</androidx.constraintlayout.widget.ConstraintLayout>