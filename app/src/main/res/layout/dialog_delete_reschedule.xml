<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <TextView
            android:id="@+id/tvDelete_DialogDeleteRschdl"
            style="@style/normaltxt_18spsemibold"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:gravity="center"
            android:text="Delete"
            android:textAlignment="center"
            android:textColor="@color/new_black"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:id="@+id/divider13"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvDelete_DialogDeleteRschdl" />

        <TextView
            android:id="@+id/tvReschedule_DialogDeleteRschdl"
            style="@style/normaltxt_18spsemibold"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:gravity="center"
            android:text="Reschedule"
            android:textAlignment="center"
            android:textColor="@color/new_black"
            app:layout_constraintEnd_toEndOf="@+id/divider13"
            app:layout_constraintStart_toStartOf="@+id/divider13"
            app:layout_constraintTop_toBottomOf="@+id/divider13" />

        <View
            android:id="@+id/divider14"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvReschedule_DialogDeleteRschdl" />

        <TextView
            android:id="@+id/tvCancle_DialogDeleteRschdl"
            style="@style/normaltxt_18spsemibold"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_marginBottom="24dp"
            android:gravity="center"
            android:text="Cancel"
            android:textAlignment="center"
            android:textColor="@color/new_black"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/divider14"
            app:layout_constraintStart_toStartOf="@+id/divider14"
            app:layout_constraintTop_toBottomOf="@+id/divider14" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>